{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "plugin:react/recommended",
      "standard"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "comma-dangle": ["warn", "always-multiline"],
      "prefer-arrow-callback": "warn",
      "arrow-parens": ["error", "as-needed"],
      "eol-last": "warn"
  }
}